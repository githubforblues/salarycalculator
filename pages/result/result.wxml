<!--pages/result/result.wxml-->
<view class='backgroundcolor'>
  <view class='salaryref_border'>
    <view class='salaryref extra_margin_left'>
      <image src='../../images/salaryref@3x.png' class='salaryref_img'></image>
      <view class='salaryref_text'>你的月薪参考</view>
    </view>
    <view class='salarynum extra_margin_left'>
      <view class='yuan_big'>¥</view>
      <view class='salary_number'>{{salaryref}}</view>
      <view class='salary_yuan'>元</view>
    </view>
  </view>
  <view class='salary_range'>
    <view class='yuan_m'>¥</view>
    <view class='salary_range_text'>{{mouthPayBeg}} ～ {{mouthPayEnd}} 元</view>
  </view>

  <view class='beat'>
    <view class='beatround'>
      <view class='beatround_text'>你击败了</view>
      <view class='beatround_number'>{{rankOfAll}}%</view>
      <view class='beatround_name'>全国应届毕业生</view>
    </view>
    <view class='beatround'>
      <view class='beatround_text'>你击败了</view>
      <view class='beatround_number'>{{rankOfCity}}%</view>
      <view class='beatround_name'>{{city}}市应届毕业生</view>
    </view>
  </view>
  
  <view class='surplusref_and_img'>

    <view>
      <view class='surplusref_border'>
        <view class='surplusref extra_margin_left'>
          <image src='../../images/surplusref@3x.png' class='surplusref_img'></image>
          <view class='surplusref_text'>月结余参考</view>
        </view>
        <view class='surplusnum extra_margin_left'>
          <view class='yuan_big_gary'>¥</view>
          <view class='surplus_number'>{{surplusref}}</view>
          <view class='surplus_yuan'>元</view>
        </view>
      </view>

      <view class='surplus_range'>
        <view class='surplus_yuan_big'>¥</view>
        <view class='surplus_range_text'>{{balanceBeg}} ～ {{balanceEnd}} 元</view>
      </view>

      <view class='return_text'>根据你的月结余你被评为</view>

    </view>

    <image src='../../avatar/level{{levelnum}}.png' class='return_img'></image>

  </view>
  <view class='levellist'>
    <view class='level'>
      <view class="level_name {{assessLevel == '乞丐' ? 'level_selected' : ''}}">乞丐级</view>
      <image src='../../images/level1_qigai_hl@3x.png' class="level_img level1_img {{assessLevel == '乞丐' ? 'level_selected' : ''}}"></image>
    </view>
    <view class='level'>
      <view class="level_name {{assessLevel == '穷人' ? 'level_selected' : ''}}">穷人级</view>
      <image src='../../images/level2_qiongren_hl@3x.png' class="level_img {{assessLevel == '穷人' ? 'level_selected' : ''}}"></image>
    </view>
    <view class='level'>
      <view class="level_name {{assessLevel == '小康' ? 'level_selected' : ''}}">小康级</view>
      <image src='../../images/level3_xiaokang_hl@3x.png' class="level_img {{assessLevel == '小康' ? 'level_selected' : ''}}"></image>
    </view>
    <view class='level'>
      <view class="level_name {{assessLevel == '中产' ? 'level_selected' : ''}}">中产级</view>
      <image src='../../images/level4_zhongchan_hl@3x.png' class="level_img {{assessLevel == '中产' ? 'level_selected' : ''}}"></image>
    </view>
    <view class='level'>
      <view class="level_name {{assessLevel == '富豪' ? 'level_selected' : ''}}">富豪级</view>
      <image src='../../images/level5_fuhao_hl@3x.png' class="level_img level5_img {{assessLevel == '中产' ? 'level_selected' : ''}}"></image>
    </view>
  </view>

  <view class='long_dotted_line'></view>

  <button class='order'>预约个人版</button>

  <view class='private_version_info'>个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于个人版将于</view>
  <view style='width:100%; height:200px; background-color: #61a189'></view>
</view>