/* 最高学历选择栏显示控制 */
.isHide {
  display: none !important;
}
.rs_ok_or_cancel {
  display: flex;
  flex-direction: row;

  border-bottom-style: solid;
  border-bottom-color: #D4D4D4;
  border-bottom-width: 1px;

  height: 49px;
}
.rs_cancel_buttom {
  font-family: PingFangSC-Regular;
  font-size: 15px;
  color: #454545;
  letter-spacing: 0;
  margin-left: 16px;
  margin-top: 14px;
  flex: 1;
}
.rs_ok_buttom {
  font-family: PingFangSC-Regular;
  font-size: 15px;
  color: #49A388;
  letter-spacing: 0;
  text-align: right;
  flex: 1;
  margin-right: 16px;
  margin-top: 14px;
}
.rs_choice view {
  font-family: PingFangSC-Regular;
  font-size: 19px;
  letter-spacing: 0;

  padding-top: 10px;
  padding-bottom: 10px;
  text-align: center;
}
.chosen {
  color: #49A388;
}
.notchosen {
  color: #A4A4A4;
}
.extra_ir_textstyle {
  font-size: 22px !important;
  width: 110px !important;
}


/* 输入数字防止点击其他选项 */
.selectlockshade {
  position: absolute;
  z-index: 30;
  top: 0;
  left: 0;
  opacity: 0.2;
  background-color: #000000;
  width: 100vw;
  height: 100vh;
}
.selectlock {
  display: none;
}






